{
    "tables": {
        "BookLoan": {
            "primary_key": "loan_id",
            "fields": {
                "loan_id": {
                    "type": "id"
                },
                "book_id": {
                    "type": "id",
                    "ref": {
                        "field": "book_id",
                        "table": "Book"
                    }
                },
                "student_id": {
                    "type": "id",
                    "ref": {
                        "field": "student_id",
                        "table": "Student"
                    }
                },
                "loan_date": {
                    "type": "datetime",
                    "format": "%Y-%m-%d"
                },
                "return_date": {
                    "type": "datetime",
                    "format": "%Y-%m-%d"
                }
            }
        },
        "Book": {
            "primary_key": "book_id",
            "fields": {
                "book_id": {
                    "type": "id"
                },
                "title": {
                    "type": "categorical"
                },
                "author": {
                    "type": "categorical"
                },
                "isbn": {
                    "type": "categorical"
                },
                "library_id": {
                    "type": "id",
                    "ref": {
                        "field": "library_id",
                        "table": "Library"
                    }
                }
            }
        },
        "Library": {
            "primary_key": "library_id",
            "fields": {
                "library_id": {
                    "type": "id"
                },
                "name": {
                    "type": "categorical"
                },
                "location": {
                    "type": "categorical"
                }
            }
        },
        "Student": {
            "primary_key": "student_id",
            "fields": {
                "student_id": {
                    "type": "id"
                },
                "name": {
                    "type": "categorical"
                },
                "email": {
                    "type": "categorical"
                },
                "date_of_birth": {
                    "type": "datetime",
                    "format": "%Y-%m-%d"
                },
                "major": {
                    "type": "categorical"
                }
            }
        },
        "Enrollment": {
            "primary_key": "enrollment_id",
            "fields": {
                "enrollment_id": {
                    "type": "id"
                },
                "student_id": {
                    "type": "id",
                    "ref": {
                        "field": "student_id",
                        "table": "Student"
                    }
                },
                "course_id": {
                    "type": "numerical",
                    "subtype": "integer"
                },
                "semester": {
                    "type": "categorical"
                },
                "grade": {
                    "type": "categorical"
                }
            }
        },
        "Submission": {
            "primary_key": "submission_id",
            "fields": {
                "submission_id": {
                    "type": "id"
                },
                "assignment_id": {
                    "type": "numerical",
                    "subtype": "integer"
                },
                "student_id": {
                    "type": "id",
                    "ref": {
                        "field": "student_id",
                        "table": "Student"
                    }
                },
                "submission_date": {
                    "type": "datetime",
                    "format": "%Y-%m-%d %H:%M:%S"
                },
                "grade": {
                    "type": "categorical"
                }
            }
        }
    }
}